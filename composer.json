{
  "name": "miloshavlicek/doctrine-api-mapper",
  "description": "REST API Doctrine entity mapper",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "friendsofsymfony/rest-bundle": ">=2.3",
    "nelmio/cors-bundle": "dev-master",
    "symfony/translation": ">=4.0",
    "symfony/console": ">=4.0",
    "symfony/dotenv": ">=4.0",
    "sensio/framework-extra-bundle": ">=5.1",
    "symfony/expression-language": ">=4.0",
    "symfony/flex": ">=1.0",
    "symfony/orm-pack": ">=1.0",
    "symfony/property-info": ">=4.0",
    "symfony/security-bundle": ">=4.0",
    "symfony/serializer-pack": ">=1.0",
    "symfony/validator": ">=4.0",
    "symfony/yaml": ">=4.0"
  },
  "suggest": {
    "phpoffice/phpspreadsheet": "For exports to xlsx.",
    "dompdf/dompdf": "For exports to PDF (LGPL) - Prefered.",
    "tecnickcom/tcpdf": "For exports to PDF (LGPL) - Alternative.",
    "mpdf/mpdf": "For exports to PDF (GPL) - Alternative only for open-source."
  },
  "require-dev": {
    "kutny/tracy-bundle": ">=1.11"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Miloshavlicek\\DoctrineApiMapper\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Miloshavlicek\\DoctrineApiMapper\\Tests\\": "tests/"
    }
  },
  "conflict": {
    "symfony/symfony": "*"
  }
}
